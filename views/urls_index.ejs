
<!DOCTYPE html>
<html lang="en">
<head>
  <% include partials/_bootstrapStyles %>
  <title>Tiny App</title>
</head>

<body>
<% include partials/_header %>
<ul>
<% Object.keys(urls).forEach(function(url) { %>
  <li>
  <%= url %> --> <a href="<%= urls[url] %>" target="_new"><%= urls[url] %></a>
  <a href="/urls/<%= url %>">Edit</a>
  <form method="POST" action="/urls/<%= url %>/delete">
    <button type="submit">Delete</button>
  </form>
  </li>
<% }); %>
</ul>

</body>

</html
